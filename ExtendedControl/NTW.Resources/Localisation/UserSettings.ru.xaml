<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:y="clr-namespace:System;assembly=mscorlib"
                    xmlns:data="clr-namespace:NTW.Data.Models;assembly=NTW.Data">

    <y:String x:Key="UserSettingsLogo">Пользовательские настройки</y:String>
    <y:String x:Key="UserSettingsUserName">Имя пользователя</y:String>
