                    xmlns:data="clr-namespace:NTW.Data.Models;assembly=NTW.Data">

    <!--AppSettingsTemplate-->
    <y:String x:Key="AppSettingsCaption">App Settings</y:String>
    <y:String x:Key="AppSettingsLanguage">Language</y:String>
    <y:String x:Key="AppSettingsTheme">Theme</y:String>
    
    <y:String x:Key="AppSettingsMainBackColor">Main back color</y:String>
