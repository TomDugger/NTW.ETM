<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:y="clr-namespace:System;assembly=mscorlib">

    <!--Panel-->
    <y:String x:Key="ProcessAddButton" >Add new app</y:String>
    <y:String x:Key="ProcessEditButton">Edit</y:String>
    <y:String x:Key="ProcessRemoveButton">Remove</y:String>
    
    <!--Control-->
    <y:String x:Key="ProcessControlProcessName">Process name</y:String>
    <y:String x:Key="ProcessControlPathToProcess">Path to process</y:String>
    <y:String x:Key="ProcessControlAccepted">Accepted</y:String>
    <y:String x:Key="ProcessControlCancel">Cancel</y:String>
    <y:String x:Key="ProcessControlFolder">Folder</y:String>
    
    <!--Errors-->
    <y:String x:Key="ProcessControlStringNotIsath">String not is path</y:String>

    <y:String x:Key="ProcessControlCaptionIsNotNull">The Caption field can not be empty</y:String>
    <y:String x:Key="ProcessControlPathIsNotNull">The Path field can not be empty</y:String>
</ResourceDictionary>